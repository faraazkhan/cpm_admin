    d.setDate(d.getDate()-1);

    function selectAllServers(me){
	       if (me == '')
        		return;
        	else {
				var button = me

				var search = document.getElementById("GetServer");
				if(button.value == 'SelectAll') {
					var NumOptions =  search.length;
					for (indice = 0; indice < NumOptions; indice++) {
						search.options[indice].selected = true;
					}
					button.value = 'UnselectAll';
				} else {
					var NumOptions =  search.length;
					for (indice = 0; indice < NumOptions; indice++) {
						search.options[indice].selected = false;
					}
					search.options[0].selected = true;
					button.value = 'SelectAll';
				}
	       }

    }

    function selectAllStorageServers(me){
	       if (me == '')
        		return;
        	else {
				var button = document.getElementById("GetStorageData");
				var search = document.getElementById("GetServer");
				if(button.value == 'Get All Servers Data') {
					var NumOptions =  search.length;
					for (indice = 0; indice < NumOptions; indice++) {
						search.options[indice].selected = true;
					}
					button.value = 'Unselect All Servers Data';
				} else {
					var NumOptions =  search.length;
					for (indice = 0; indice < NumOptions; indice++) {
						search.options[indice].selected = false;
					}
					search.options[0].selected = true;
					button.value = 'Get All Servers Data';
				}
	       }

    }



    function update_select_options( target, opts_array, clear_select_list ) {

        if( $(target).type.match("select" ) ){ // Confirm the target is a select box

            // Remove existing options from the target and the clear_select_list
            clear_select_list[clear_select_list.length] = target // Include the target in the clear list

            for( k = 0;k < clear_select_list.length;k++){
                obj = $(clear_select_list[k]);
                if( obj.type.match("select") ){
                    len = obj.childNodes.length;
                    for( var i=0;i < len;i++){obj.removeChild(obj.firstChild);}
                }
            }

            // Populate the new options
            for(i=0;i < opts_array.length;i++){
                o = document.createElement( "option" );
                o.appendChild( document.createTextNode( opts_array[i][0] ) );
                o.setAttribute( "value", opts_array[i][1] );
                o.setAttribute( "title", opts_array[i][0] );
                if(i==0) o.setAttribute("selected",true);
                obj.appendChild(o);
            }

        }
    }

    function submit_storage_mtf() {
        document.GetGraph.action = "<%= @capacity_storage_mtf_link %>";
