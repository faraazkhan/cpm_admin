                      <% else %>
                          <table border="0" id="type_report_performance" style="display: none;">
                        <% end %>
                          <tr>
                            <td width="32"><span class="style1 style10">Daily:</span></td>
                            <td width="96">
                              <input name="datereport" id="datereport" type="text" class="style10" onClick="this.value=formatDate(d,'y-MM-dd');cal.select(this,'datereport','yyyy-MM-dd'); return false;">
                            </td>
                            <td width="66"><span class="style10">Last_Week:</span></td>
                            <td width="20"><input name="typereport" id="typereport" type="radio" class="style10" value="LastWeek" onclick="this.form.datereport.value='';this.form.submit()"></td>
                            <td width="88"><span class="style10">Current_month:</span></td>
                            <td width="20"><input name="typereport" id="typereport" type="radio" class="style10" value="CurrentMonth" onclick="this.form.datereport.value='';this.form.submit()"></td>
