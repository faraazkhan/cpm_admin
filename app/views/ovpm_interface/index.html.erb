                            <td width="66"><input name="typereport" id="typereport" type="radio" class="style10" value="LastMonth" onclick="this.form.datereport.value='';this.form.submit()"></td>
                            <td><input type="submit" name="Submit" value="GetGraph" class="style10"></td>
                          </tr>
                        </table>
                        <% if @capacity_table and !@global_table %>
                          <table border="0" id="type_report_capacity">
