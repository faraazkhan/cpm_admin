                              <% else %>
                                  <input name="<%= metric.name %>" id="<%= metric.name %>" type="checkbox" class="style10" onclick="this.form.submit();">
                              <% end %>
                              </td>
                            <% end %>
                            </tr>
                            <% if @capacity_table and !@global_table %>
                              <tr id="capacity_metrics" style="display: block;">
                            <% else %>
                                <tr id="capacity_metrics" style="display: none;">
                              <% end %>
                                <td class="style10">Cpu_Usage_Forecast<input name="metric" id="mectric" type="radio" value="CpuForecast" class="style10" onclick="this.form.submit();" checked></td>
                                <td class="style10">Cpu_Usage<input name="metric" id="mectric" type="radio" value="CpuUsage" class="style10" onclick="this.form.submit();"></td>
